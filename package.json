{
  "name": "megatar",
  "description": "Create full offline Helm chart packages.",
  "version": "0.0.0-development",
  "author": "Felipe Santos @felipecassiors",
  "bin": {
    "megatar": "./bin/run"
  },
  "bugs": "https://github.com/felipecassiors/megatar/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-autocomplete": "^0.2.0",
    "@oclif/plugin-help": "^3.2.0",
    "@oclif/plugin-not-found": "^1.2.4",
    "chalk": "^4.1.0",
    "execa": "^4.0.3",
    "listr": "^0.14.3",
    "shelljs": "^0.8.4",
    "tslib": "^1.13.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@oclif/dev-cli": "^1.22.2",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/execa": "^2.0.0",
    "@types/jest": "^26.0.9",
    "@types/listr": "^0.14.2",
    "@types/node": "^10.17.28",
    "@types/shelljs": "^0.8.8",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "eslint-config-prettier": "^6.11.0",
    "globby": "^10.0.2",
    "jest": "^26.2.2",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "semantic-release": "^17.1.1",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/felipecassiors/megatar",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "megatar",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-autocomplete"
    ]
  },
  "repository": "felipecassiors/megatar",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc jest",
    "version": "oclif-dev readme && git add README.md",
    "release": "semantic-release"
  },
  "types": "lib/index.d.ts",
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "tarballDir": "dist"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "README.md",
            "CHANGELOG.md",
            "package.json",
            "package-lock.json",
            "npm-shrinkwrap.json"
          ]
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": "dist/*.tgz"
        }
      ]
    ]
  }
}
